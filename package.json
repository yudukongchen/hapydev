  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "start": "vite --mode development",
    "start:docker": "docker-compose up -d",
    "build": "vite build",
    "build:pre-docker": "docker buildx create --name mybuilder --use",
    "preview:docker": "docker build -t hapydev/hapydev:latest  .",
    "build:docker": "docker buildx build --push --platform linux/amd64,linux/arm64  -t hapydev/hapydev:latest  .",
    "dev": "concurrently \"vite\" \"wait-on http://localhost:8000 && electron .\"",
    "electron:build": "vite build && electron-builder"
